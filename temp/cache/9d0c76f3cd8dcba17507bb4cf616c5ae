imlinefeeds";a:7:{s:8:"required";b:0;s:4:"name";s:14:"Trim Linefeeds";s:11:"description";s:97:"Remove the linefeeds added after each input which casues the wiki parser to add extra paragraphs.";s:5:"since";s:4:"14.1";s:7:"options";a:3:{i:0;a:2:{s:4:"text";s:0:"";s:5:"value";s:0:"";}i:1;a:2:{s:4:"text";s:2:"No";s:5:"value";s:1:"0";}i:2;a:2:{s:4:"text";s:3:"Yes";s:5:"value";s:1:"1";}}s:6:"filter";s:6:"digits";s:7:"default";i:0;}s:15:"searchable_only";a:7:{s:8:"required";b:0;s:4:"name";s:23:"Searchable Only Results";s:11:"description";s:55:"Only include results marked as searchable in the index.";s:5:"since";s:4:"14.1";s:7:"options";a:3:{i:0;a:2:{s:4:"text";s:0:"";s:5:"value";s:0:"";}i:1;a:2:{s:4:"text";s:2:"No";s:5:"value";s:1:"0";}i:2;a:2:{s:4:"text";s:3:"Yes";s:5:"value";s:1:"1";}}s:6:"filter";s:6:"digits";s:7:"default";i:1;}s:14:"customsearchjs";a:7:{s:8:"required";b:0;s:4:"name";s:33:"Use custom search JavaScript file";s:11:"description";s:117:"Mainly keeps the search state on the URL hash, but also adds some helper functions like easier sorting and page size.";s:5:"since";s:4:"14.1";s:7:"options";a:3:{i:0;a:2:{s:4:"text";s:0:"";s:5:"value";s:0:"";}i:1;a:2:{s:4:"text";s:2:"No";s:5:"value";s:1:"0";}i:2;a:2:{s:4:"text";s:3:"Yes";s:5:"value";s:1:"1";}}s:6:"filter";s:6:"digits";s:7:"default";i:0;}}}i:52;a:11:{s:4:"help";N;s:4:"name";s:5:"CYPHT";s:5:"title";s:13:"Cypht webmail";s:13:"documentation";s:11:"PluginCypht";s:11:"description";s:36:"Embed Cypht webmail and news reader.";s:5:"prefs";a:1:{i:0;s:16:"wikiplugin_cypht";}s:8:"iconname";s:8:"envelope";s:10:"introduced";s:4:"20.0";s:6:"format";s:4:"html";s:4:"tags";a:1:{i:0;s:5:"basic";}s:6:"params";a:24:{s:9:"imap_name";a:6:{s:4:"name";s:20:"IMAP connection name";s:11:"description";s:47:"Name for the IMAP connection. E.g. "My Mailbox"";s:8:"required";b:0;s:7:"default";s:0:"";s:6:"filter";s:4:"text";s:5:"since";s:4:"20.0";}s:11:"imap_server";a:6:{s:4:"name";s:11:"IMAP server";s:11:"description";s:21:"E.g. imap.example